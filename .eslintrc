{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "require-jsdoc": "off",
    "valid-jsdoc":"off",
    "linebreak-style": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "object-curly-spacing":"off",
    "css-semicolonexpected": "off",
    "operator-linebreak": "off",
    "no-debugger": "off",
    "no-case-declarations": "off"
  },
  "extends": ["eslint:recommended", "google"]
}